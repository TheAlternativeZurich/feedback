{
  "name": "TheAlternativeZurich/feedback",
  "description": "collect feedback from lectures & workshop with minimal setup & instant evaulation ",
  "license": "MIT",
  "require": {
    "php": "^7.2",
    "ext-iconv": "*",
    "ext-json": "*",
    "ext-pdo": "*",
    "ext-pdo_sqlite": "*",
    "doctrine/doctrine-fixtures-bundle": "^3.0",
    "php-coveralls/php-coveralls": "^2.0",
    "ramsey/uuid": "^3",
    "sensio/framework-extra-bundle": "^5.1",
    "sensiolabs/security-checker": "^5.0",
    "symfony/apache-pack": "^1.0",
    "symfony/asset": "^4.1",
    "symfony/console": "^4.1",
    "symfony/dependency-injection": "^4.1",
    "symfony/dotenv": "^4.2",
    "symfony/expression-language": "^4.1",
    "symfony/flex": "^1.0",
    "symfony/form": "^4.1",
    "symfony/framework-bundle": "^4.1",
    "symfony/maker-bundle": "^1.4",
    "symfony/monolog-bundle": "^3.1",
    "symfony/orm-pack": "^1.0",
    "symfony/property-info": "^4.1",
    "symfony/requirements-checker": "^1.0",
    "symfony/security-bundle": "^4.1",
    "symfony/serializer": "^4.1",
    "symfony/swiftmailer-bundle": "^3.1",
    "symfony/translation": "^4.1",
    "symfony/twig-bundle": "^4.1",
    "symfony/validator": "^4.1",
    "symfony/webpack-encore-pack": "^1.0",
    "symfony/yaml": "^4.1"
  },
  "require-dev": {
    "codacy/coverage": "^1.4",
    "deployer/deployer": "^6.1",
    "friendsofphp/php-cs-fixer": "^2.7",
    "fzaninotto/faker": "^1.7",
    "symfony/browser-kit": "^4.1",
    "symfony/css-selector": "^4.1",
    "symfony/debug-bundle": "^4.1",
    "symfony/phpunit-bridge": "^4.1",
    "symfony/profiler-pack": "^1.0",
    "symfony/web-server-bundle": "^4.1"
  },
  "config": {
    "platform": {
      "php": "7.2"
    },
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  },
  "replace": {
    "symfony/polyfill-iconv": "*",
    "symfony/polyfill-php70": "*",
    "symfony/polyfill-php56": "*"
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd",
      "security:check": "symfony-cmd",
      "doctrine:migrations:migrate -q": "symfony-cmd",
      "requirements-checker": "script"
    },
    "post-install-cmd": [
      "@auto-scripts"
    ],
    "post-update-cmd": [
      "@auto-scripts"
    ]
  },
  "conflict": {
    "symfony/symfony": "*"
  }
}
